{
  "name": "@ekgrahn/halo-api",
  "version": "1.1.0",
  "description": "Node wrapper for the Halo API. Returns a Promise that will return JSON from the API response.",
  "main": "./src/halo.js",
  "scripts": {
    "test": "mocha --recursive --exit",
    "lint": "./node_modules/.bin/eslint ./src/",
    "docs": "run-s docs:clear docs:generate",
    "docs:generate": "./node_modules/.bin/jsdoc -R ./README.md -c ./conf.json -t ./node_modules/ink-docstrap/template -d ./docs -r ./src/",
    "docs:clear": "rm -r ./docs"
  },
  "author": "ekgrahn",
  "license": "ISC",
  "devDependencies": {
    "chai": "4.2.0",
    "dotenv": "4.0.0",
    "eslint": "7.16.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "ink-docstrap": "1.3.2",
    "jsdoc": "3.6.7",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EthanGrahn/halo-api.git"
  },
  "files": [
    "src/*"
  ],
  "dependencies": {
    "axios": "0.21.1"
  }
}
